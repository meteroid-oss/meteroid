  "private": true,
  "scripts": {
    "build": "tsc && vite build && pnpm generate:css",
    "build-storybook": "pnpm generate:css && storybook build --quiet -o build",
    "chromatic": "chromatic --auto-accept-changes",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "dev": "concurrently \"pnpm generate:css -- --watch\" \"vite\"",
    "generate:css": "npx tailwindcss -i ./src/styles/tailwind.css -o ./dist/tailwind.css && node remove-styles.js",
    "postinstall": "pnpm generate:css",
    "prepack": "json -f package.json -I -e \"delete this.devDependencies; delete this.dependencies\"",
    "preview": "vite preview",
    "storybook": "concurrently \"pnpm generate:css -- --watch\" \"storybook dev -p 6006\""
